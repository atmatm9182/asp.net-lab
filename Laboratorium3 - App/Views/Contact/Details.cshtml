@using lab3_App.Models
@model Contact
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Szczegoly</h1>

<h2>Imię</h2>
<p>@Model.Name</p>

<h2>Email</h2>
<p>@Model.Email</p>

@if (@Model.Phone != null)
{
    <h2>Numer telefonu</h2>
    <p>@Model.Phone</p>
}

@if (@Model.Birth != null)
{
<h2>Data urodzenia</h2>
<p>@Model.Birth</p>
}

<button class="btn btn-primary" asp-action="Index">Wroc</button>
